<h1> Comment on the Emag</h1>

<%= form_for(@comment) do |f| %>
    
	<div class="field">
		<%= f.label :comment %><br>
		<%=f.text_field :comment %>

    </div>
    <div class="field">
		<%= f.label :user_id %><br>
		<%=f.text_field :user_id %>

    </div>
    <div class="field">
		<%= f.label :emag_id %><br>
		<%=f.text_field :emag_id %>
	
    </div>
    <div class="action">
      <%= f.submit %>
    </div>
    
    
    
    
<% end %>